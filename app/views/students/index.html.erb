<div class="row">
  <div class="col s12">
    <h1 class="center-align">Students listing</h1>

  </div>
</div>

<div class="row">
  <% @students.each do |student| %>
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= student.name%></span>
          <span>E-mail: <%= student.email %></span>

          <p>Course enrollments: none</p>
        </div>
        <div class="card-action">
          <%= link_to "View profile", student_path(student) %>
        </div>
      </div>
    </div>
  <% end %>
</div>


